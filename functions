
#include<stdio.h>//scanf(), printf()
#include<math.h>//sin(),cos()
int sum(int x, int y, int z);//function prototype 
float division(float a, float b);//function prototype 

int main(){
int result,a,b,c;
float div_1;

  printf("Enter 3 values:");
  scanf("%d%d%d", &a,&b,&c);
  
  result=sum(a,b,c);//function calling 
  div_1=division(25,20);// function calling 
  
  printf("The sum is %d\n", result);
  printf("The division is %.2f\n",div_1);
  return 0;
  }
  
  //function definition 
  int sum(int x, int y, int z){
  int result;
  result=x+y+z;
  return result;
  }
  //function definition 
  float division(float a, float b){
  float div;
  div=a/b;
  return div;
  }
  